{
    "name": "maesbox/video-bundle",
    "type": "symfony-bundle",
    "description": "Video Bundle adding functiunalities to Sonata media bundle:\n\=> a video provider\n\    - handle video upload\n\   - re-encode uploaded video to th defined formats\n\   - thumbs generation",
    "keywords": ["video", "bundle", "media", "ffmpeg", "sonata"],
    "homepage": "http://github.com/maerianne/MaesboxVideoBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maerianne",
            "email": "maerianne@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "sonata-project/media-bundle": "dev-master",
        "phansys/getid3": "2.1.*@dev"
        
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1"
    },
    "autoload": {
        "psr-0": { "Maesbox\\VideoBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Maesbox/VideoBundle"
}